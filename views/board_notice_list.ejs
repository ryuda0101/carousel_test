<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>공지사항</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <%- include("common/header.ejs") %>
    <div id="board_list_wrap">
        <div class="center">
            <table class="brd_top">
                <tr class="brd_top">
                    <th class="number">번호</th>
                    <th class="auther">분류</th>
                    <th >제목</th>
                    <th class="date">날짜</th>
                </tr>
                <!-- 반복문으로 데이터 받아와서 여기에 넣어주기 -->
                <% brdData.forEach(function(el, index){ %>
                    <tr class="brd_bot">
                        <th class="number"><%- el.num %></th>
                        <th class="classification"><%- el.classification %></th>
                        <th class="title user_title"><a href="/detail/<%- el.num %>"><%- el.title %></a></th>
                        <th class="date user_date"><%- el.date %></th>
                    </tr>
                <% }); %>
            </table>
        </div>
    </div>
    <%- include("common/footer.ejs") %>
</body>
</html>